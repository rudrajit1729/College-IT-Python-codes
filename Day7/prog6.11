x = []
while(True):
    c = int(raw_input("1. Insert \n2. Delete \n3. Display"))
    if c == 1:
        inp = raw_input("Enter:")
        x.insert(0, inp)
    elif c == 2:
        x.pop(0)
    elif c == 3:
        print(x)
    else:
        print("Wrong input")
    ch = raw_input("Wanna continue?[y/n]:")
    if ch == 'n':
        break
    
    
